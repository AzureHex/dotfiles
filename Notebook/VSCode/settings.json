{
    "update.mode": "none",
    "breadcrumbs.enabled": false,
    "files.autoSave": "afterDelay",
    "material-icon-theme.hidesExplorerArrows": true,
    "remoteHub.commitDirectlyWarning": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",

    "codesnap.containerPadding": "8em",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 12px 24px",
    "codesnap.backgroundColor": "#FFC540",
    "codesnap.showLineNumbers": false,
    "codesnap.roundedCorners": true,

    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,

    "editor.guides.indentation": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.horizontal": "hidden",
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontSize": 17,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.hideCursorInOverviewRuler": true,
    "editor.hover.delay": 1500,
    "editor.stickyScroll.enabled": true,

    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,

    "window.customTitleBarVisibility": "never",
    "window.newWindowDimensions": "inherit",
    "window.nativeFullScreen": true,
    "window.zoomLevel": 1,
    "window.newWindowProfile": "eyes",
    "window.commandCenter": false,

    "workbench.activityBar.location": "default",
    "workbench.colorTheme": "Catppuccin Macchiato",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.editor.showTabs": "single",
    "workbench.layoutControl.enabled": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.enablePreview": true,
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 20,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ffe4b5" // Replace with your desired color code
        },
    "vscode_custom_css.imports": [
        "file:///C:/Users/eyes/.vscode/vscode.css"
    ],
