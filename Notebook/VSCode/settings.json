{
    "update.mode": "none",
    "breadcrumbs.enabled": false,
    "files.autoSave": "afterDelay",
    "material-icon-theme.hidesExplorerArrows": true,
    "monospace.fontFamily": "'JetBrains Mono Nerd Font', monospace",
    "remoteHub.commitDirectlyWarning": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",

    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,

    "editor.guides.indentation": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.horizontal": "hidden",
    "editor.fontSize": 17,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.hideCursorInOverviewRuler": true,
    "editor.hover.delay": 1500,
    "editor.stickyScroll.enabled": true,
    "editor.fontFamily": "'JetBrains Mono Nerd Font', monospace",
    "editor.titleBar.enabled": false,

    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "window.newWindowDimensions": "inherit",
    "window.nativeFullScreen": true,
    "window.zoomLevel": 1,

    "workbench.activityBar.location": "default",
    "workbench.colorTheme": "Catppuccin Macchiato",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.editor.showTabs": "single",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.enablePreview": true,
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 20,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ffe4b5", // Replace with your desired color code
        "titleBar.forground": "#24273a",
        "titleBar.activeForeground": "#24273a",
        "titleBar.background": "#24273a",
        "titleBar.activeBackground": "#24273a",
        },
    "vscode_custom_css.imports": [
        "file:///C:/Users/eyes/vscode.css",
        "file:///C:/Users/eyes/vscode.js"
    ],
    "vscode_custom_css.policy": true,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
